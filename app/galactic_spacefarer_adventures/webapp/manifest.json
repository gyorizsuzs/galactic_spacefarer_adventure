{
  "_version": "1.65.0",
  "sap.app": {
    "id": "galactic.adventures.galacticspacefareradventures",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.16.5",
      "toolsId": "8313f02e-c262-4e8f-9981-d2a9bf5bddae"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/spacefarer/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.133.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "galactic_spacefarer_adventures.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "flexibleColumnLayout": {
          "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
          "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
        },
        "routerClass": "sap.f.routing.Router"
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "GalacticSpacefarersList",
          "target": [
            "GalacticSpacefarersList"
          ]
        },
        {
          "pattern": "GalacticSpacefarers({key}):?query:",
          "name": "GalacticSpacefarersObjectPage",
          "target": [
            "GalacticSpacefarersList",
            "GalacticSpacefarersObjectPage"
          ]
        }
      ],
      "targets": {
        "GalacticSpacefarersList": {
          "type": "Component",
          "id": "GalacticSpacefarersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/GalacticSpacefarers",
              "variantManagement": "Page",
              "navigation": {
                "GalacticSpacefarers": {
                  "detail": {
                    "route": "GalacticSpacefarersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "growingTreshold": 10,
                    "growingMode": "Scroll",
                    "enableAutoColumnWidth": false,
                    "showFullScreenButton": true,
                    "personalization": {
                      "filter": true,
                      "column": true,
                      "sort": true
                    },
                    "quickVariantSelection": {
                      "showCounts": true
                    }
                  },
                  "filters": {
                    "defaultFilterMode": "liveMode"
                  },
                  "columns": {
                    "fullName": {
                      "label": "{{fullName}}",
                      "position": 10,
                      "filterable": true,
                      "sortable": true
                    },
                    "originPlanet": {
                      "label": "{{originPlanet}}",
                      "position": 20,
                      "filterable": true,
                      "sortable": true
                    },
                    "stardustCollection": {
                      "label": "{{stardustCollection}}",
                      "position": 30,
                      "filterable": true,
                      "sortable": true
                    },
                    "wormholeNavigationSkill": {
                      "label": "{{wormholeNavigationSkill}}",
                      "position": 40,
                      "filterable": true,
                      "sortable": true
                    },
                    "spacesuitColour": {
                      "label": "{{spacesuitColour}}",
                      "position": 50,
                      "filterable": true,
                      "sortable": true
                    },
                    "department": {
                      "label": "{{department}}",
                      "position": 60,
                      "filterable": true,
                      "sortable": true
                    },
                    "position": {
                      "label": "{{position}}",
                      "position": 70,
                      "filterable": true,
                      "sortable": true
                    }
                  },
                  "actions": {
                    "DataFieldForAction::SpacefarerService.EntityContainer::CreateSpacefarer": {
                      "afterExecution": {
                        "enableAutoScroll": true
                      }
                    }
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          },
          "controlAggregation": "beginColumnPages",
          "contextPattern": ""
        },
        "GalacticSpacefarersObjectPage": {
          "type": "Component",
          "id": "GalacticSpacefarersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/GalacticSpacefarers",
              "viewLevel": 2,
              "content": {
                "header": {
                  "anchorBarVisible": true
                }
              },
              "sectionLayout": "Tabs"
            }
          },
          "controlAggregation": "midColumnPages",
          "contextPattern": "/GalacticSpacefarers({key})"
        }
      }
    },
    "rootView": {
      "viewName": "sap.fe.templates.RootContainer.view.Fcl",
      "type": "XML",
      "async": true,
      "id": "appRootView"
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}