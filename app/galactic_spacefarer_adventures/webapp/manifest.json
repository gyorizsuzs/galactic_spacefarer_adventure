{
  "_version": "1.65.0",
  "sap.app": {
    "id": "galactic.adventures.galacticspacefareradventures",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.16.5",
      "toolsId": "8313f02e-c262-4e8f-9981-d2a9bf5bddae"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/spacefarer/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.133.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "galactic.adventures.galacticspacefareradventures.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "GalacticSpacefarersList",
          "target": "GalacticSpacefarersList"
        },
        {
          "pattern": "GalacticSpacefarers({key}):?query:",
          "name": "GalacticSpacefarersObjectPage",
          "target": "GalacticSpacefarersObjectPage"
        }
      ],
      "targets": {
        "GalacticSpacefarersList": {
          "type": "Component",
          "id": "GalacticSpacefarersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/GalacticSpacefarers",
              "variantManagement": "Page",
              "navigation": {
                "GalacticSpacefarers": {
                  "detail": {
                    "route": "GalacticSpacefarersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "enableAutoColumnWidth": true,
                    "showFullScreenButton": true
                  },
                  "filters": {
                    "defaultFilterMode": "liveMode"
                  },
                  "columns": {
                    "name": {
                      "label": "Name",
                      "position": 10,
                      "filterable": true,
                      "sortable": true
                    },
                    "originPlanet": {
                      "label": "Origin Planet",
                      "position": 20,
                      "filterable": true,
                      "sortable": true
                    },
                    "rank": {
                      "label": "Rank",
                      "position": 30,
                      "filterable": true,
                      "sortable": true
                    }
                  }
                }
              }
            }
          }
        },
        "GalacticSpacefarersObjectPage": {
          "type": "Component",
          "id": "GalacticSpacefarersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/GalacticSpacefarers"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}